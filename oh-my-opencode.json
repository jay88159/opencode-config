{
  "$schema": "https://raw.githubusercontent.com/code-yeongyu/oh-my-opencode/master/assets/oh-my-opencode.schema.json",
  "agents": {
    "sisyphus": {
      "model": "deepseek/deepseek-reasoner",
      "variant": "medium"
    },
    "oracle": {
      "model": "openai/gpt-5.2",
      "variant": "medium"
    },
    "librarian": {
      "model": "opencode/big-pickle"
    },
    "explore": {
      "model": "opencode/gpt-5-nano"
    },
    "multimodal-looker": {
      "model": "google/gemini-3-flash"
    },
    "prometheus": {
      "model": "anthropic/claude-opus-4-5-20251101",
      "variant": "medium"
    },
    "metis": {
      "model": "openai/gpt-5.2",
      "variant": "medium"
    },
    "momus": {
      "model": "openai/gpt-5.2",
      "variant": "medium"
    },
    "atlas": {
      "model": "anthropic/claude-opus-4-5-20251101"
    },
    "researcher": {
      "model": "deepseek/deepseek-reasoner"
    },
    "writer": {
      "model": "deepseek/deepseek-reasoner"
    },
    "diagrammer": {
      "model": "deepseek/deepseek-reasoner"
    },
    "reviewer": {
      "model": "deepseek/deepseek-reasoner"
    }
  },
  "categories": {
    "visual-engineering": {
      "model": "google/gemini-3-pro"
    },
    "writing": {
      "model": "deepseek/deepseek-reasoner"
    }
  },
  "skills": {
    "doc-writing": {
      "description": "文档写作编导：通过 task 工具按 category='writing' 调度 doc-researcher、doc-writer、doc-diagrammer、doc-reviewer 四个 worker skill",
      "agent": "sisyphus"
    },
    "doc-researcher": {
      "description": "文档调研员：Web 搜索（全英文 query）、素材采集、引用整理",
      "subtask": true
    },
    "doc-writer": {
      "description": "文档撰稿：正文写作、占位符管理、内容融合",
      "subtask": true
    },
    "doc-diagrammer": {
      "description": "文档图表师：Mermaid 图表、DrawIO 图表生成",
      "subtask": true
    },
    "doc-reviewer": {
      "description": "文档审校：质量检查、段落重写、最终润色",
      "subtask": true
    },
    "sources": [
      "~/.config/opencode/skills"
    ]
  },
  "_migrations": [
    "model-version:openai/gpt-5.2-codex->openai/gpt-5.3-codex",
    "model-version:openai/gpt-5.2-codex->openai/gpt-5.3-codex",
    "model-version:openai/gpt-5.2-codex->openai/gpt-5.3-codex"
  ]
}
